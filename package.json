{
  "name": "wanago-typescript-express",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev ./src/server.ts --env-file .env",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -d ./src/data-source.ts",
    "test": "jest",
    "build": "tsc",
    "start": "NODE_ENV=production npm run build && node ./dist/server.js",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/$npm_config_name -d ./src/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d ./src/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.7.2",
    "otplib": "^12.0.1",
    "pg": "^8.13.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.7.5",
    "@types/qrcode": "^1.5.6",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-lib": "^0.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "type": "commonjs"
}
